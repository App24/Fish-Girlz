    <DefineConstants>DEV</DefineConstants>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="SFML.Net" Version="*"/>
    <PackageReference Include="DiscordRichPresence" Version="*"/>
  </ItemGroup>
</Project>